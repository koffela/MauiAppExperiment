<?xml version="1.0" encoding="utf-8" ?>
<!-- DiscordPage.xaml -->
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MauiAppExperiment.DiscordPage"
             BackgroundImageSource = "tyt.png">
    <StackLayout Padding="20">
        <Label Text="Discord" FontSize="28" HorizontalOptions="Center"/>
        <Entry x:Name="DiscordEntry" Placeholder="Say something..." PlaceholderColor="Aqua"/>
        <Button x:Name="DiscordButton" Text="Send Message" BackgroundColor="Aqua"/>
    </StackLayout>
</ContentPage>